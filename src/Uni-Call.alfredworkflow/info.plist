				<key>script</key>
				<string>query={query}

case "$query" in
	"[CTSkype]"*)
		osascript bin/skypecall.scpt ${query#'[CTSkype]'};;
	"[CTFaceTime]"*)
		open "facetime:${query#'[CTFaceTime]'}";;
	"[CTPhoneAmego]"*)
		open "phoneAmego:${query#'[CTPhoneAmego]'};alert=no";;
	"[CTSIP]"*)
		query="${query#'[CTSIP]'}"
		open "${query//[ -()]/}";;
	"[CTPushDialer]"*)
		open "pushdialer://${query#'[CTPushDialer]'}";;
	"[CTGrowlVoice]"*)
		open "growlvoice:${query#'[CTGrowlVoice]'}?call";;
esac</string>
